;;;  Hogyoku App Launcher Window
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

(defwindow app_launcher
  :monitor 0
  :geometry (geometry :x "0%"
                      :y "10%"
                      :width "30%"
                      :height "40%")
  :stacking "overlay"
  :focusable true
  :onclose "eww update launcher_reveal=false"
  (launcher))
